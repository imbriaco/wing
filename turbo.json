{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "compile": {
      "dependsOn": ["^compile"]
    },
    "dev": {
      "dependsOn": ["^compile"],
      "persistent": true,
      "cache": false
    },
    "test": {
      "dependsOn": ["^compile"],
      "cache": false
    },
    "post-compile": {},
    "lint": {
      "outputs": []
    },
    "eslint": {
      "outputs": []
    },
    "package": {
      "dependsOn": ["compile"],
      "cache": false
    }
  }
}
