{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "pipeline": {
    "pre-compile": {
      "cache": false
    },
    "compile": {
      "dependsOn": ["pre-compile"],
      "inputs": ["src/**"],
      "outputs": ["lib/**", ".jsii"]
    },
    "post-compile": {
      "dependsOn": ["compile"],
      "cache": false
    }
  }
}
